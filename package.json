{
    "name": "vscode-auto-timestamp",
    "displayName": "Auto Timestamp",
    "description": "Auto update timestamp field on save",
    "version": "0.0.1",
    "publisher": "lpubsppop01",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "configuration": [
            {
                "title": "Auto Timestamp configuration",
                "type": "object",
                "properties": {
                    "lpubsppop01.autoTimestamp.lineLimit": {
                        "type": "integer",
                        "default": 5
                    },
                    "lpubsppop01.autoTimestamp.birthTimeStart": {
                        "type": "string",
                        "default": "Created: "
                    },
                    "lpubsppop01.autoTimestamp.birthTimeEnd": {
                        "type": "string",
                        "default": "$"
                    },
                    "lpubsppop01.autoTimestamp.modifiedTimeStart": {
                        "type": "string",
                        "default": "Last modified: "
                    },
                    "lpubsppop01.autoTimestamp.modifiedTimeEnd": {
                        "type": "string",
                        "default": "$"
                    },
                    "lpubsppop01.autoTimestamp.momentFormat": {
                        "type": "string",
                        "default": "YYYY/MM/DD HH:mm:ss"
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "moment": "^2.19.3"
    }
}
